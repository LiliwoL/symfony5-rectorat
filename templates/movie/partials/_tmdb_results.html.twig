<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    {% for result in results %}
        <tr>
        <th scope="row">{{ result.id }}</th>
        <td>{{ result.original_title }}</td>
